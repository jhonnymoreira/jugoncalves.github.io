{
  "name": "jugoncalves.github.io",
  "version": "0.1.0",
  "description": "jugoncalves' blog",
  "repository": {
    "type": "git",
    "url": "git://github.com/jugoncalves/jugoncalves.github.io.git"
  },
  "author": "Jú Gonçalves <github@jugoncalv.es>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jugoncalves/jugoncalves.github.io/issues"
  },
  "scripts": {
    "js": "./node_modules/.bin/browserify src/js/index.js > assets/js/main.js",
    "css": "./node_modules/.bin/stylus < src/styl/main.styl > assets/css/main.css",
    "min": "./node_modules/.bin/uglifyjs assets/js/main.js -o assets/js/main.min.js",
    "bundle": "npm run js; npm run css;"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "homepage": "http://jugoncalv.es",
  "dependencies": {
    "browserify": "^7.0.0",
    "drf.colors": "~0.1.0",
    "drf.grid": "~1.0.0",
    "jeet": "^6.1.2",
    "lodash": "^2.4.1",
    "mocha": "^2.0.1",
    "nib": "~1.0.2",
    "react": "^0.12.1",
    "reactify": "^0.17.1",
    "stylus": "^0.49.3",
    "superagent": "^0.21.0",
    "uglify-js": "^2.4.16",
    "underscore": "^1.7.0"
  }
}
